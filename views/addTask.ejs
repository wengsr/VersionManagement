<form action="/task/addTask" id="formAddTask" class=" bs-component form-horizontal" method='post'>
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title" id="myLargeModalLabel">申请变更单</h4>
    </div>
    <div  class="modal-body">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <input type="hidden" id="inputTasker" name="inputTasker" value="翁舒嵘"/>

                    <div class="form-group">
                        <label for="inputTaskName" class="col-sm-2 control-label">产品</label>
                        <div class="col-sm-10">
                            <select class="form-control">
                                <option>NCRM</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputTaskName" class="col-sm-2 control-label">变更单名称</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control"  name="inputTaskName" id="inputTaskName" placeholder="变更单名称">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputTaskDesc" class="col-sm-2 control-label">变更内容描述</label>

                        <div class="col-sm-10">
                            <textarea class="form-control" id="inputTaskDesc" rows="3"></textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputTaskList" class="col-sm-2 control-label">变更文件清单</label>

                        <div class="col-sm-10">
                            <textarea class="form-control" id="inputTaskList" rows="3"></textarea>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
        <button id="btnSubmit" type="button" class="btn btn-primary" data-dismiss="modal">提交</button>
    </div>
</form>