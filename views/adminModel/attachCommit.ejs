<form action="" id="formCommitRar" class=" bs-component form-horizontal" method='post'>
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span>
            <span class="sr-only">Close</span></button>
        <h4 class="modal-title" id="myLargeModalLabel"><span style="color: #cccccc">上传变更单至svn</span></h4>
    </div>
    <div  class="modal-body">
        <div class="container-fluid">
            <div class="row">
                <!--<div class="col-lg-1"></div>-->
                <div class="col-lg-11">

                    <div class="form-group">
                        <label for="inputTaskList" class="col-sm-2 control-label">附件名</label>
                        <div class="col-sm-10">
                            <div >
                                <a id="attachmentId" attachmentId =<%=attachment.attachmentId%> name="attachmentName" ><%= attachment.fileName%></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <div style="width:59%;float:left;margin-bottom:-20px;margin-top:-9px;height:50px;" id="diaErrTip" class="alert alert-danger alert-dismissible" role="alert"><span style="text-align:center;margin-left:auto;margin-right:auto;display:block;"><strong>错误提示</strong></span></div>
        <div style="width:59%;float:left;margin-bottom:-20px;margin-top:-9px;height:50px;" id="diaSuccessTip" class="alert alert-success alert-dismissible" role="alert"><span style="text-align:center;margin-left:auto;margin-right:auto;display:block;"><strong>操作成功</strong></span></div>
        <div  style="width:40%;float:right;">
            <button id="btnCloseModel" type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
            <button id="btnCopyRar" type="button" class="btn btn-primary" >创建上传副本</button>
            <button id="btnCommitRar"  type="button" class="btn btn-primary" >确定上传</button>
        </div>
    </div>
</form>

<script src="/javascripts/common.js"></script>
<script src="/javascripts/attachmentCommit.js"></script>
