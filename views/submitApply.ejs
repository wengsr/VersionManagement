<form action="/task/addTask" id="formAddTask" class=" bs-component form-horizontal" method='post'>
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title" id="myLargeModalLabel">申请变更单</h4>
    </div>
    <div  class="modal-body">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">

                    <div class="form-group">
                        <label for="inputTaskName" class="col-sm-2 control-label">产品</label>
                        <div class="col-sm-10">
                            <select class="form-control" name="project" id="project">
                               <!-- <option value="000001" selected >NCRM</option>
                                <option value="000002" >FCRM</option>
                                <option value="000003"  >BCRM</option>-->
                                <option value="" selected >--请选择--</option>
                                <%if(project && project.length>0){%>
                                <%project.forEach(function(pro,i){%>
                                <option value="<%=pro.projectId%>"><%=pro.projectName%></option>
                                <%})%>
                                <%}%>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputTaskName" class="col-sm-2 control-label">变更单名称</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control"  name="inputTaskName" id="inputTaskName" placeholder="变更单名称">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputTaskDesc" class="col-sm-2 control-label">变更内容描述</label>

                        <div class="col-sm-10">
                            <textarea class="form-control" id="inputTaskDesc" rows="3" name="inputTaskDesc"></textarea>
                        </div>
                        </div>


                    <div class="form-group">
                        <label for="inputTaskList" class="col-sm-2 control-label">新增文件清单</label>

                        <div class="col-sm-10">
                            <textarea class="form-control" id="inputTaskNewList" rows="3" name="taskNewFiles"></textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputTaskModList" class="col-sm-2 control-label">修改文件清单</label>
                        <div class="col-sm-10">
                            <textarea class="form-control" id="inputTaskModList" rows="3" name="taskModFiles"></textarea>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
       <div style="width:68%;float:left;margin-bottom:-20px;margin-top:-9px;height:50px;" id="diaInfoTip" class="alert alert-info alert-dismissible" role="alert"><span style="text-align:center;margin-left:auto;margin-right:auto;display:block;"><strong>信息提示</strong></span></div>
        <div style="width:68%;float:left;margin-bottom:-20px;margin-top:-9px;height:50px;" id="diaErrTip" class="alert alert-danger alert-dismissible" role="alert"><span style="text-align:center;margin-left:auto;margin-right:auto;display:block;"><strong>错误提示</strong></span></div>
        <div style="width:68%;float:left;margin-bottom:-20px;margin-top:-9px;height:50px;" id="diaSuccessTip" class="alert alert-success alert-dismissible" role="alert"><span style="text-align:center;margin-left:auto;margin-right:auto;display:block;"><strong>操作成功</strong></span></div>
        <button type="button"  id ="closeModel" class="btn btn-default" data-dismiss="modal">关闭</button>
        <button id="submitApply" type="button" class="btn btn-primary" >提交</button>
        <button id="btnSubmitSuccess" type="button" class="btn btn-primary" >提交成功</button>
    </div>
</form>
<script src="/javascripts/submitApply.js"></script>
<script src="/javascripts/common.js"></script>