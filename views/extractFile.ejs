<!--手动提取文件(该界面已不用，被自动提取代替)-->
<form action="/extractFile/acceptTask" id="formExtractFile" class=" bs-component form-horizontal" method='post'>
    <input type="hidden" id="taskId" name="taskId" value="<%= task.taskid%>"/>
    <input type="hidden" id="processStepId" name="processStepId" value="<%= task.processStepId%>"/>
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
        <h4 class="modal-title" id="myLargeModalLabel">手动提取文件</h4>
    </div>
    <div  class="modal-body">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">

                    <div class="form-group">
                        <label for="inputTaskName" class="col-sm-2 control-label">变更单号</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control"  name="inputTaskName" id="inputTaskName" placeholder="变更单名称" value="<%=task.taskname%>">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputTaskName" class="col-sm-2 control-label">变更单名</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control"  name="inputTaskName" id="inputTaskName" placeholder="变更单名称">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputTaskDesc" class="col-sm-2 control-label">申请人</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control"  name="inputTaskName" id="inputTaskName" placeholder="变更单名称">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputTaskList" class="col-sm-2 control-label">变更文件清单</label>
                        <div class="col-sm-10">
                            <textarea class="form-control" id="inputTaskList" rows="3" disabled="disabled">fdafdsa</textarea>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <!--<div style="width:78%;float:left;margin-bottom:-20px;margin-top:-9px;height:50px;" id="diaErrTip" class="alert alert-danger alert-dismissible" role="alert"><span style="text-align:center;margin-left:auto;margin-right:auto;display:block;"><strong>错误提示</strong></span></div>-->
        <div style="width:78%;float:left;margin-bottom:-20px;margin-top:-9px;height:50px;" id="diaSuccessTip" class="alert alert-success alert-dismissible" role="alert"><span style="text-align:center;margin-left:auto;margin-right:auto;display:block;"><strong>操作成功</strong></span></div>
        <div style="width:20%;float:right;">
            <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
        <% if("null" == task.dealerName){ %>
            <button id="acceptMission" type="button" class="btn btn-primary" data-dismiss="modal">接受任务</button>
        <% }else{ %>
            <button id="btnSubmit" type="button" class="btn btn-primary" data-dismiss="modal">提取完成</button>
        <% } %>
        </div>
    </div>
</form>
<script src="/javascripts/extractFile.js"></script>